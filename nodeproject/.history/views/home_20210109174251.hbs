<header>
    {{#if isLoggedIn}}
    <h1 class="logo">{{logout}}</h1>
    {{else}}
    <h1 class="logo">{{logoin}}</h1>
    {{/if}}
    <div>
        {{#if ifReady}}
        {{else}}
        <button type="button">버튼</button>
        {{/if}}
    </div>
    {{#each users}}
        <span>{{this}}</span>
    {{/each}}
    {{#each users as |value key|}}
        <span>{{value}}</span>
        <span>{{key}}</span>
    {{/each}}
</header>