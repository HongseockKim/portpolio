%!PS-Adobe-3.0 Resource-CompatibleFont
%%BeginResource: CompatibleFont (BRDOTM)
%%Title: (CompatibleFont/BRDOTM)
%%Version: 4.001
%%DocumentNeededResources: ProcSet (AdobeTypeComposer)
%%+ ProcSet (CompatibleFonts)
%%Creator: Yoon Design Inc.
%%Copyright: Copyright (C) 1989-2004 Yoon Design Inc. All Rights Reserved.
%%Copyright: All rights reserved.
%%EndComments

/BRDOTM dup

/CIDInit /ProcSet resourcestatus
{ pop pop /CIDInit /ProcSet findresource true }
{ /AdobeTypeComposer /ProcSet resourcestatus
  { pop pop /AdobeTypeComposer /ProcSet findresource true
  }
  { false
  }
 ifelse
}
ifelse

{ begin
    [/BRDOTM-KSCpc-EUC-H] 
    beginrearrangedfont

    endrearrangedfont
  end
}
{ pop pop /findfont errordict /invalidfont get exec stop }
ifelse

/CompatibleFont defineresource pop

%%EndResource
%%EOF
